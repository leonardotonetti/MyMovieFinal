                    </dt>
                    <dd>
                        @Html.DisplayFor(model => model.DataCadastro)
                    </dd>
                    <dt>
                        @Html.DisplayNameFor(model => model.AlteracaoUsuarioId)
                    </dt>
                    <dd>
                        @Html.DisplayFor(model => model.AlteracaoUsuarioId)
                    </dd>
                    <dt>
                        @Html.DisplayNameFor(model => model.DataAlteracao)
                    </dt>
                    <dd>
                        @Html.DisplayFor(model => model.DataAlteracao)
                    </dd>
                    <dt>
                        @Html.DisplayNameFor(model => model.Ativo)
                    </dt>
                    <dd>
                        @Html.DisplayFor(model => model.Ativo)
                    </dd>
                </dl>
            </div>
            <div class="validationErrors">
                @Html.ValidationSummary()
            </div>
            <div>
                @Html.ActionLink("Editar", "Editar", new { id = Model.CategoriaId }) |
                <a asp-action="Categorias">Voltar</a>
            </div>

        </div>
    </div>
</div>